<%= form_for(@questions) do |f| %>
  <%= render 'shared/error_messages', object: f.object %>
  <div class="field">
    <%= f.text_area :title, placeholder: "Title of question..." %>
    <%= f.text_area :body, placeholder: "Body of question..." %>
  </div>
  <%= f.submit "Post", class: "btn btn-primary" %>
<% end %>